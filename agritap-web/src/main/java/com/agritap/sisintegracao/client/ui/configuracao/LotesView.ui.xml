<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:d="urn:import:org.gwtbootstrap3.client.ui.gwt" 
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:l="urn:import:com.google.gwt.core.ext.linker"
	xmlns:b2="urn:import:org.gwtbootstrap3.extras.datepicker.client.ui">
	<g:HTMLPanel>
	
	<b:Row>
		<b:Column size="XS_12">
				<b:Heading size="H1" styleName="page-header" text="Configuração do Lote" />
			<b:Panel >
				<b:PanelHeader>
					<b:Heading size="H3"  text="Dados Gerais" />
				</b:PanelHeader>
				<b:PanelBody>
				    <b:Form>
						<b:FieldSet>
						    <b:FormGroup ui:field="produtor">
						      <b:FormLabel for="formProdutor">Produtor</b:FormLabel>
						      <b:FormControlStatic b:id="produtorLabel"></b:FormControlStatic>
					        </b:FormGroup>
						
							<b:InputGroup>
							  <b:TextBox placeholder="Pesquisar" autoComplete="false" b:id="pesquisa"/>
							  <b:InputGroupAddon icon="SEARCH" iconPosition="RIGHT"></b:InputGroupAddon>
							</b:InputGroup>
						   
						      
						    <b:FormGroup>  
						      <b:FormLabel for="formQuantidade">Intervalo de datas</b:FormLabel>
						      <b:Row>
						      	<b:Column size="XS_6">
		                     	 <b2:DatePicker autoClose="true" ui:field="de"/>
						      	</b:Column>
						      	<b:Column size="XS_6">
	                              
		                     	 <b2:DatePicker autoClose="true" ui:field="ate"/>
								</b:Column>
								</b:Row>                              
		                    </b:FormGroup>
		                   
						    <b:FormGroup>  
		                    </b:FormGroup>
					    </b:FieldSet>
					</b:Form>
  				</b:PanelBody>
			</b:Panel>
		</b:Column>
		
	</b:Row>
	<b:Row>
		<b:Column size="XS_12">
			<d:CellTable bordered="true" condensed="true" striped="true" hover="true" ui:field="resultados"/>
		</b:Column>
	</b:Row>
		<g:HTMLPanel ui:field="addLote" styleName="addButton">
			<span>+</span>
		</g:HTMLPanel>
</g:HTMLPanel>

</ui:UiBinder> 