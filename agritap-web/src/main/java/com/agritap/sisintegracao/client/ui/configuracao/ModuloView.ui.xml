<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:l="urn:import:com.google.gwt.core.ext.linker" xmlns:b2="urn:import:org.gwtbootstrap3.extras.datepicker.client.ui">


	<g:HTMLPanel>
		<b:Row>
			<b:Column size="XS_12">
				<b:Heading size="H1" styleName="page-header" text="Modulo" />
			</b:Column>
		</b:Row>

		<b:Row ui:field="formRow" >
			<b:Column size="XS_12">
				<b:Panel>
					<b:PanelBody>

						<b:ListGroup ui:field="errorBox" visible="false">
						</b:ListGroup>
						
						<b:Form ui:field="form">
							<b:FieldSet>

								<b:Row>
									<b:Column size="XS_12,SM_6">
										<b:FormGroup>
											<b:FormLabel for="produtorField"> Produtor </b:FormLabel>
											<b:ListBox ui:field= "produtorField" />
										</b:FormGroup>
									</b:Column>
									
									<b:Column size="XS_12,SM_6">
										<b:FormGroup>
											<b:FormLabel for="tipoAnimalField"> Tipo do Animal </b:FormLabel>
											<b:ListBox ui:field="tipoAnimalField" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="XS_12,SM_6">
										<b:FormGroup>
											<b:FormLabel for="alojamentoMaximoField"> Alojamento Maximo </b:FormLabel>
											<b:TextBox placeholder="Alojamento Maximo" ui:field="alojamentoMaximoField" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Row>
									<b:Column size="XS_12,SM_6">
										<b:FormGroup>
											<b:FormLabel for="nomeBarracaoField"> Nome do Barracão </b:FormLabel>
											<b:TextBox placeholder="Nome do barracão" ui:field="nomeBarracaoField" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								
								<b:Button type="PRIMARY" ui:field="salvarBtn">Salvar</b:Button>
								<b:Button addStyleNames="col-xs-offset-1" ui:field="cancelarBtn">Cancelar</b:Button>
								<b:Button type="DANGER" addStyleNames="col-xs-offset-1"
									ui:field="excluirBtn">Excluir</b:Button>
							</b:FieldSet>
						</b:Form>
					</b:PanelBody>
				</b:Panel>
			</b:Column>
		</b:Row>
	
	</g:HTMLPanel>

</ui:UiBinder> 