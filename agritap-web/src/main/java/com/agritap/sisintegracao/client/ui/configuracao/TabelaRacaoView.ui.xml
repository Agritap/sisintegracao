<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:d="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:gwt="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:l="urn:import:com.google.gwt.core.ext.linker"
	xmlns:b2="urn:import:org.gwtbootstrap3.extras.datepicker.client.ui"
	xmlns:ag="urn:import:com.agritap.sisintegracao.client.ui.component">



	<g:HTMLPanel>
		<b:Row>
			<b:Column size="XS_12">
				<b:Heading size="H1" styleName="page-header" text="Tabela Ração" />
			</b:Column>
		</b:Row>

		<b:Row ui:field="formRow" visible="true">
			<b:Column size="XS_12">
				<b:Panel>
					<b:PanelBody>

						<b:ListGroup ui:field="errorBox" visible="false">
						</b:ListGroup>
						<b:Form ui:field="form">
							<b:FieldSet>

								<b:Row>
									<b:Column size="XS_12,SM_6">
										<b:FormGroup>
											<b:FormLabel for="dataInicioField"> Data inicio </b:FormLabel>
											<b2:DatePicker placeholder="Data inicio"
												ui:field="dataInicio" format="dd-mm-yyyy" language="PT_BR" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="XS_12,SM_6">
										<b:FormGroup>
											<b:FormLabel for="dataFimField"> Data fim </b:FormLabel>
											<b2:DatePicker placeholder="Data fim" ui:field="dataFim"
												format="dd-mm-yyyy" language="PT_BR" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="XS_12,SM_6">
										<b:FormGroup>
											<b:FormLabel for="pesoField"> Peso minimo </b:FormLabel>
											<ag:BigDecimalBox placeholder="Peso Minimo"
												ui:field="pesoMinimo" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Button type="PRIMARY" ui:field="salvarBtn">Salvar</b:Button>
							</b:FieldSet>
						</b:Form>
					</b:PanelBody>
				</b:Panel>
			</b:Column>
		</b:Row>
	</g:HTMLPanel>

</ui:UiBinder> 