<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:l="urn:import:com.google.gwt.core.ext.linker"
	xmlns:b2="urn:import:org.gwtbootstrap3.extras.datepicker.client.ui">

	<b:Row>
		<b:Column size="XS_12">
				<b:Heading size="H1" styleName="page-header" text="Vacinação dos animais" />
			<b:Panel >
				<b:PanelBody>
				    <b:Form>
						<b:FieldSet>
						    <b:FormGroup>
						      <b:FormLabel for="formProdutor">Produtor</b:FormLabel>
						      <b:FormControlStatic b:id="formProdutor">Mauro (pego a partir do usuario logado)</b:FormControlStatic>
					        </b:FormGroup>
					        
					        
						
						    <b:FormGroup>  
						      <b:FormLabel for="formQuantidade"> Data da Vacinação </b:FormLabel>
		                      <b2:DatePicker autoClose="true"/>
                              <b:Modal title="DatePicker in Modal" closable="true" fade="true" dataBackdrop="STATIC" dataKeyboard="true" b:id="myModal" ui:field="modal1">  
                              <b:ModalBody>
                              <b2:DatePicker/>
                              </b:ModalBody>
                              <b:ModalFooter>
                              <b:Button type="DANGER" dataDismiss="MODAL">Close</b:Button>
                              </b:ModalFooter>
                              </b:Modal>
		                    </b:FormGroup>
						     
						     
						     
						     <b:FormGroup>
						      <b:FormLabel for="formQuantidade"> Quantidade de animais que serão vacinados</b:FormLabel>	
						      			
						      <b:TextBox placeholder="" b:id="formQuantidade">  </b:TextBox>
                            </b:FormGroup>
                            
						    <b:FormGroup> 
						      
						      
						      
						      
						      
						      
						    </b:FormGroup>
					   						 
                        </b:FieldSet>
                     </b:Form>
                  </b:PanelBody>
              </b:Panel>
          </b:Column>
                        
		<b:Column size="XS_12">
			    	<b:Button>Confirmar</b:Button>
	    			</b:Column>	
	</b:Row>
</ui:UiBinder> 